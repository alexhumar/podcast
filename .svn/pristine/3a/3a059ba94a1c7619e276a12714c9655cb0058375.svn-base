@import url("estilo2.css");

#pie{
	clear:both;
}

.sf_admin_actions li{
	float:left;
	margin-left: 5%;
}

.sf_admin_pagination img{
	width:5%;
}

#subCuerpo{
	width:88%;
	height:100%; 
}

#menuV{
	float:left;
	width:18%;
	height:100%;
	background-color:#202020;
}

#cuerpo{
	width:100%;
	min-height:90%;
	height:90%;
	float:left;
}

.sf_admin_filter{
	float:right;
	padding-right:2%;
}


#menuV a:link, #menuV a:visited{
	display:block;
	text-align:left;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 70%;
	background-color:#B0C4DE;
	border-bottom:solid;
	border-color:#202020;
	color:black;
	text-decoration:none;
	width:100%;	
	padding-top:10%;
	padding-bottom:10%; 
}
#menuV a:hover{
	background-color: #202020;
	color:white;
	font-weight: bold;
}

#bienvenida{
	float:left;
}

.formu{
	clear:left;
	line-height:50px;
	margin:10px;
}

.cancelar{
	margin-left:20px;
}

.lista{
	clear:left;
	line-height:20px;
	margin:10px;
}

.lista a:visited, .lista a:link{
	color:blue;
}

.lista a:hover{
	color:red;
}
