	padding-left: 185px;
    padding-top: 10px;
}

#podcast_filters_nombre{
	width:150px;
}

#podcast_filters_descripcion{
	width:150px;
}

ul {
    list-style: none; 
}

#sf_admin_container{
	color:lavender;
}

#sf_admin_container a{
	color:goldenRod;
}
 
